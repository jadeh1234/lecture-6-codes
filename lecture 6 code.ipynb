   "source": [
    "#Create a PDF object\n",
    "pdf = FPDF(orientation='P', unit='mm', format='A4')\n",
    "\n",
    "#Add a page\n",
    "pdf.add_page()\n",
    "\n",
    "#Set font\n",
    "pdf.set_font('Arial', 'B', 16)\n",
    "\n",
    "#Add text\n",
    "pdf.cell(40, 10, 'Hello World')\n",
    "\n",
    "#Save PDF\n",
    "pdf.output('hello_world.pdf')\n",
    "\n"
   ]
